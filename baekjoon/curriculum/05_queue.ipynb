    "from collections import deque\n",
    "\n",
    "\n",
    "cards = deque(range(1, int(input())+1))\n",
    "\n",
    "while len(cards) > 1:\n",
    "    cards.popleft()\n",
    "    cards.rotate(-1)\n",
    "\n",
    "print(cards[0])"
   ]
